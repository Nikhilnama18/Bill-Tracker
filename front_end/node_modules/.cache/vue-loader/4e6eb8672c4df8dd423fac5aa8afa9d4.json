word :\n    <input\n      v-model=\"u_pwd1\"\n      type=\"password\"\n      id=\"u_password\"\n      placeholder=\"Password\"\n    />\n    <br />\n    Re-Enter Password\n    <input\n      v-model=\"u_pwd2\"\n      type=\"password\"\n      id=\"u_password1\"\n      placeholder=\"Re-Enter Password\"\n    />\n  </div>\n  <Button @click=\"create\" title=\"Create\" />\n  <Button @click=\"goBack\" title=\"Cancel\" color=\"red\" />\n</template>\n<script>\nimport Button from \"./Button.vue\";\nexport default {\n  name: \"NewUser\",\n  data() {\n    return {\n      u_name: \"\",\n      u_pwd1: \"\",\n      u_pwd2: \"\",\n    };\n  },\n  components: {\n    Button,\n  },\n  methods: {\n    create() {\n      if (this.u_pwd1 != this.u_pwd2) {\n        alert(\"Passwords are not same \");\n        return;\n      }\n      if (this.u_name == \"\" || this.u_pwd1 == \"\") {\n        alert(\"Please Enter all details\");\n        return;\n      }\n      if (this.u_pwd1.length <=5) {\n        alert(\"Password is to weak\");\n        return;\n      }\n      this.$emit(\"create\", { u_name: this.u_name, u_password: this.u_pwd1 });\n    },\n    goBack() {\n      this.$emit(\"goBack\");\n    },\n  },\n  emits: [\"create\", \"goBack\"],\n};\n</script>\n\n<style >\n.userDetails {\n  background: #f4f4f4;\n  margin: 5px;\n  padding: 10px 20px;\n  cursor: pointer;\n}\n</style>"]}]}