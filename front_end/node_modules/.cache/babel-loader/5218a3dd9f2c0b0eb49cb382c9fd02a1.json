AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACZ,OAAO,CAAC,mDAAD,CADK;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAES,KAAK,qBACb,YAAY,CAAC,OAAb,CAAqB,MAArB,CADa,mBACwB,KAAI,CAAC,GAAL,CAAS,IADjC,GAE1B;AACE,kBAAA,MAAM,EAAE,KADV;AAEE,kBAAA,OAAO,EAAE;AACP,oCAAgB,kBADT;AAEP,oBAAA,aAAa,mBAAY,YAAY,CAAC,OAAb,CAAqB,UAArB,CAAZ;AAFN,mBAFX;AAME,kBAAA,IAAI,EAAE,IAAI,CAAC,SAAL,CAAe;AACnB,oBAAA,IAAI,EAAE,YAAY,CAAC,OAAb,CAAqB,MAArB,CADa;AAEnB,oBAAA,IAAI,EAAE,KAAI,CAAC,GAAL,CAAS,IAFI;AAGnB,oBAAA,MAAM,EAAE,KAAI,CAAC,UAHM;AAInB,oBAAA,KAAK,EAAE,KAAI,CAAC,SAJO;AAKnB,oBAAA,UAAU,EAAE,KAAI,CAAC;AALE,mBAAf;AANR,iBAF0B,CAFd;;AAAA;AAER,gBAAA,QAFQ;;AAmBd,oBAAI,QAAQ,CAAC,MAAT,KAAoB,GAAxB,EAA6B;AAC3B,kBAAA,KAAI,CAAC,eAAL,GAAuB,KAAvB;AACA,kBAAA,KAAI,CAAC,GAAL,CAAS,MAAT,GAAkB,KAAI,CAAC,UAAvB;AACA,kBAAA,KAAI,CAAC,GAAL,CAAS,UAAT,GAAsB,KAAI,CAAC,cAA3B;AACA,kBAAA,KAAI,CAAC,GAAL,CAAS,KAAT,GAAiB,KAAI,CAAC,SAAtB;AACF;;AAxBc;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA0BjB;AAxCM,GAhBI;AA0Db,EAAA,KAAK,EAAE,CAAC,WAAD,EAAc,WAAd;AA1DM,CAAf","sourcesContent":["<template>\n  <div class=\"org\">\n    <div class=\"bill-buttons\">\n      <button\n        class=\"edit-button\"\n        style=\"font-size: 120%\"\n        @click=\"isUpdateClicked = !isUpdateClicked\"\n        alt=\"Edit Bill\"\n      >\n        <i\n          :class=\"['fa', isUpdateClicked ? 'fa-times' : 'fa-pencil']\"\n          aria-hidden=\"true\"\n        ></i>\n      </button>\n      <button\n        class=\"delete-button\"\n        style=\"font-size: 120%\"\n        @click=\"deleteOrg\"\n        alt=\"Delete Bill\"\n      >\n        <i class=\"fa fa-trash\" aria-hidden=\"true\"></i>\n      </button>\n    </div>\n\n    <div @click=\"renderBills\" v-show=\"!isUpdateClicked\">\n      <label>Org Name : </label>\n      {{ org.o_name }}\n      <p></p>\n      <label> Location : </label>\n      {{ org.o_location }}\n      <p></p>\n      <label>GST Number : </label>\n      {{ org.o_gst }}\n    </div>\n\n    <div v-show=\"isUpdateClicked\">\n      <label> Org Name : </label>\n      <input\n        class=\"linkField\"\n        type=\"text\"\n        name=\"update_Name\"\n        v-model=\"updateName\"\n      />\n      <p></p>\n      <label> Location : </label>\n      <input\n        class=\"linkField\"\n        type=\"text\"\n        name=\"update_Location\"\n        v-model=\"updateLocation\"\n      />\n      <p></p>\n      <label> GST Number: </label>\n      <input\n        class=\"linkField\"\n        type=\"text\"\n        name=\"update_GST_Number\"\n        v-model=\"updateGST\"\n      />\n      <Button @click=\"updateOrg\" title=\"Update\" color=\"purple\" />\n    </div>\n  </div>\n</template>\n\n\n<script>\nimport Button from \"./Button.vue\";\nexport default {\n  name: \"Organisation\",\n  data() {\n    return {\n      isUpdateClicked: false,\n      updateName: this.org.o_name,\n      updateLocation: this.org.o_location,\n      updateGST: this.org.o_gst,\n    };\n  },\n  components: {\n    Button,\n  },\n  props: {\n    org: Object,\n  },\n  methods: {\n    renderBills() {\n      this.$router.push({\n        name: \"Bills\",\n        params: { o_id: this.org.o_id, o_name: this.org.o_name },\n      });\n      localStorage.setItem(\"org_id\", this.org.o_id);\n      localStorage.setItem(\"org_name\", this.org.o_name);\n    },\n    deleteOrg() {\n      if (confirm(\"Are You sure ? You wanna delete this Organization\")) {\n        this.$emit(\"deleteOrg\", this.org.o_id);\n      }\n    },\n    async updateOrg() {\n      if (confirm(\"Are You sure ? You wanna Update this Organization\")) {\n        const response = await fetch(\n          `api/users/${localStorage.getItem(\"u_id\")}/orgs/${this.org.o_id}`,\n          {\n            method: \"PUT\",\n            headers: {\n              \"Content-type\": \"application/json\",\n              authorization: `Bearer ${localStorage.getItem(\"jwtToken\")}`,\n            },\n            body: JSON.stringify({\n              u_id: localStorage.getItem(\"u_id\"),\n              o_id: this.org.o_id,\n              o_name: this.updateName,\n              o_gst: this.updateGST,\n              o_location: this.updateLocation,\n            }),\n          }\n        );\n        if (response.status === 200) {\n          this.isUpdateClicked = false;\n          this.org.o_name = this.updateName;\n          this.org.o_location = this.updateLocation;\n          this.org.o_gst = this.updateGST;\n        }\n      }\n    },\n  },\n  emits: [\"deleteOrg\", \"updateOrg\"],\n};\n</script>\n\n<style  scoped>\n.fas {\n  color: rgb(0, 38, 255);\n}\n.org {\n  background: #f4f4f4;\n  margin: 5px;\n  overflow: hidden;\n  min-height: 100px;\n  padding: 20px;\n  border-radius: 10px;\n}\n.linkField {\n  width: 40%;\n  padding: 3px 3px;\n  margin: 1px 0;\n  display: inline-block;\n  border: 2px solid #ccc;\n  border-radius: 10px;\n  box-sizing: border-box;\n}\n</style>"],"sourceRoot":""}]}